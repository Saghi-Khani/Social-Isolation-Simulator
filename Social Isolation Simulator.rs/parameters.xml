						defaultValue="1000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="infected_count" displayName="Infected Count" type="int" 
						defaultValue="2" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="chance_to_infect" displayName="Transmission Rate" type="float" 
						defaultValue="0.8" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="number_of_rooms" displayName="Number of rooms in each hospital" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


